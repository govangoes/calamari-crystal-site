    "prettier": "^3.3.3",
    "postcss": "^8.4.41",
    "rimraf": "^5.0.5",
    "tailwindcss": "^4.1.14",
    "vite": "^5.4.2",
    "vitest": "^3.2.4",
    "sharp": "^0.33.4"
