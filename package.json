{
  "name": "calamari-crystal-site",
  "private": true,
  "version": "0.1.0",
  "description": "GoVanGoes • Calamari Crystal — minimal Vite+React+Tailwind baseline",
  "author": "GoVanGoes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/govangoes/calamari-crystal-site.git"
  },
  "homepage": "https://github.com/govangoes/calamari-crystal-site",
  "bugs": {
    "url": "https://github.com/govangoes/calamari-crystal-site/issues"
  },
  "engines": {
    "node": "20.x",
    "npm": ">=9.0.0"
  },
  "type": "module",
  "scripts": {
    "prebuild": "node scripts/update-sitemap-lastmod.mjs && node scripts/optimize-images.mjs",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "clean": "rimraf dist",
    "img:opt": "node scripts/optimize-images.mjs",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "npm run lint && npm run format:check",
    "test": "echo 'No tests configured'"
  },
  "dependencies": {
    "lucide-react": "^0.544.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-testing-library": "^6.5.0",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.5",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.4.14",
    "vite": "^7.1.9"
  },
  "resolutions": {}
}
